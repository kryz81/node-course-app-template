    "helmet": "3.23.3",
    "mongoose": "5.13.20",
    "redis": "3.1.1",
    "swagger-jsdoc": "3.4.0",
    "swagger-ui-express": "4.5.0",
    "uuid": "3.3.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "documentation": "14.0.1",
    "eslint": "6.2.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-security": "1.4.0",
    "nodemon": "2.0.20",
    "prettier": "1.18.2"
